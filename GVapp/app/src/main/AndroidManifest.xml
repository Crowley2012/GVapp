<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="crowley.sean.GVapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gv_logo_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RapidConnect" />
        <activity android:name=".GVSU" />
        <activity android:name=".Mail" />
        <activity android:name=".Blackboard" />
        <activity android:name=".MyBanner" />
        <activity android:name=".Settings" />
        <activity android:name=".Library" />
        <activity android:name=".PeopleFinder" />
        <activity android:name=".DebitDollars"></activity>
    </application>

</manifest>
